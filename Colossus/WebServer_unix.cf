ServerPort=26766
SocketQueueLen=5
MaxClients=50
MaxUsers=500
PortRangeFrom=26767
AvailablePorts=20
UsersFile=example.users.dat
JavaCommand=/usr/bin/java
ColossusJar=/games/colossus/Colossus.jar
LogPropTemplate=/games/colossus/logging.properties
WorkFilesBaseDir=/var/games/colossus
